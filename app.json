{
  "expo": {
    "name": "Recognizr",
    "slug": "ImageRecognizer",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.singhpriyanshu.recognizr",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-7328130788754961~7096697134"
      }
    },
    "ios": {
      "bundleIdentifier": "com.singhpriyanshu.recognizr",
      "supportsTablet": true
    }
  }
}
